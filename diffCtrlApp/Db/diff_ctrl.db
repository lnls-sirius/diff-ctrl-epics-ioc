  field(INPB, "$(P)$(R)WidthStep-RB")
  field(INPC, "$(P)$(R)Width-RB")
  field(INPD, "$(CTRL_NEG)Mtr.DMOV")
  field(INPE, "$(CTRL_POS)Mtr.DMOV")
  field(CALC, "A&&D&&E")                      # Decrement command 'on' and motors not moving
  field(OCAL, "C-B")
  field(OOPT, "When Non-zero")
