  field(SELM, "All")
  field(DOL1, "$(P)$(R)Force$(NEGATIVE_EDGE_NAME)EdgePos-Cmd")
  field(DOL2, "$(P)$(R)$(POSITIVE_EDGE_NAME)EdgePos-RB")
